	_type := ADST_UINT8,
	coe_index := 16#0072,
	coe_sub_index :=16#00,
	readable := TRUE,
	writeable := FALSE,
	to_check := FALSE, (* to be evaluated *)
	CONVERSION_this := CONVERSION_this);
	
iol_slave_configure_parameter(
	current_value_address := ADR(data.system_state),
	configured_value_address := NULL,
	length := SIZEOF(data.system_state),
	_type := ADST_UINT8,
	coe_index := 16#00E2,
	coe_sub_index :=16#00,
