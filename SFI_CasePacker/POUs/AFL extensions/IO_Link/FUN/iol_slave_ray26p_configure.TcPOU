	_type := ADST_STRING,
	coe_index := 16#0017,
	coe_sub_index := 16#00,
	readable := TRUE,
	writeable := FALSE,
	to_check := FALSE,
	CONVERSION_this := CONVERSION_this);


(* CUSTOM PARAMETERS CONFIGURATION *)

iol_slave_configure_parameter(
	current_value_address := ADR(data.teach_in_state),
