(*$R process_ref:= prc; $*)

PROGRAM csp_main_safety_emergency
VAR
	prc:											process;
	prc_path:										STRING;
	
 END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[_read_var_input();

_objects_manager();

IF NOT objects_running()
THEN
	RETURN;	
END_IF

_read_process_image();

_mode_manager();

CASE process_get_mode(prc) OF
	MODE_ABSENT,
	MODE_INACTIVE:
		RETURN;
	MODE_TEST:
		;
END_CASE

_set_runtime_properties();

(********************************* BODY *********************************)
IF	descriptor_get_reset_edge(prc.descriptor)
THEN
	safety_integrated_series_close(sis);
	safety_integrated_series_close(delayed_sis);
END_IF
(************************************************************************)

_handle_messages();

_write_output();]]></ST>
    </Implementation>
