    <Action Name="_mode_manager" Id="{a61ff840-98e9-45a7-a221-a22fb9b1fd3e}">
      <Implementation>
        <ST><![CDATA[IF process_mode_req_is_pending(prc)
THEN
    process_apply_mode_req_first_step(prc);
    CASE process_get_mode(prc) OF
        MODE_TO_ABSENT:
			;
        MODE_TO_INACTIVE:
			;
        MODE_TO_TEST:
			;
        MODE_TO_OPERATIONAL:
			;
    END_CASE
    process_apply_mode_req_second_step(prc);
END_IF]]></ST>
      </Implementation>
    </Action>
    <Action Name="_objects_manager" Id="{23bcd131-068d-4171-8fbb-0098a13cd2b2}">
      <Implementation>
        <ST><![CDATA[IF objects_initialization()
THEN

	sub_process_init(
		object_name								:= 'csp_main_safety_emergency.prc', 
		process									:= csp_main_safety.prc, 
		CONVERSION_this							:= prc);
		
	prc_path 									:= process_get_path(prc);

END_IF


IF	objects_running()
THEN
    process_cycle(prc);
END_IF]]></ST>
      </Implementation>
