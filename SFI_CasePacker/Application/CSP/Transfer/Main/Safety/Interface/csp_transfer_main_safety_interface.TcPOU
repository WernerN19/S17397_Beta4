		group									:= SPLC40210_sig,
		CONVERSION_this							:= SPLC40210_sic);

	safety_integrated_controller_add_connection_interface(
		device_name								:= 'SD41300',
		connection_interface					:= _SD41300_drv_ChB,
		ethercat_master							:= csp_main.cabinet_transfer_ethercat_master,
		group									:= SPLC40210_sig,
		CONVERSION_this							:= SPLC40210_sic);
		
			safety_integrated_controller_add_connection_interface(
