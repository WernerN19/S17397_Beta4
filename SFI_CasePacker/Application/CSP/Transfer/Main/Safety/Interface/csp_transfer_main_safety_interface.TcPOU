		ethercat_master							:= csp_main.cabinet_transfer_ethercat_master,
		group									:= SPLC40210_sig,
		CONVERSION_this							:= SPLC40210_sic);
END_IF

IF objects_running()
THEN
	process_cycle(prc);
	safety_integrated_controller_cycle(SPLC40210_sic);
	safety_integrated_group_cycle(SPLC40210_sig);
END_IF]]></ST>
